{
  "tests": [
    {
      "expectedError": "KeyFormat",
      "command": {
        "messageKind": "AddProperty",
        "node": "123",
        "property": {
          "key": "",
          "language": "l1",
          "version": "1.0"
        },
        "newValue": "string"
      }
    },
    {
      "expectedError": "PropertyNull",
      "command": {
        "messageKind": "AddProperty",
        "node": "123",
        "property": {
          "key": "key",
          "language": "l1",
          "version": "1.0"
        },
        "newValue": null
      }
    },
    {
      "expectedError": "IdFormat",
      "command": {
        "messageKind": "AddProperty",
        "node": "%#&@%@",
        "property": {
          "key": "key",
          "language": "l1",
          "version": "1.0"
        },
        "newValue": "string"
      }
    },
    {
      "expectedError": "VersionFormat",
      "command": {
        "messageKind": "AddProperty",
        "node": "123",
        "property": {
          "key": "key",
          "language": "l1",
          "version": ""
        },
        "newValue": "string"
      }
    },
    {
      "expectedError": "PropertyMissing",
      "command": {
        "messageKind": "AddProperty",
        "node": "123",
        "property": {
          "key": "key",
          "language": "l1",
          "version": "22"
        }
      }
    },
    {
      "expectedError": "PropertyMissing",
      "command": {
        "messageKind": "AddProperty",
        "property": {
          "key": "key",
          "language": "l1",
          "version": "22"
        },
        "newValue": "hello"
      }
    },
    {
      "expectedError": "PropertyMissing",
      "command": {
        "messageKind": "AddProperty",
        "node": "id",
        "property": {
          "key": "key",
          "language": "l1",
          "version": "22"
        },
        "newValue": "hello",
        "protocolMessage": {
          "kind": "Info"
        }
      }
    },
    {
      "expectedError": null,
      "command": {
        "messageKind": "AddProperty",
        "node": "id",
        "property": {
          "key": "key",
          "language": "l1",
          "version": "22"
        },
        "newValue": "hello",
        "protocolMessage": {
          "kind": "Info",
          "message": "23",
          "data": {
            "a": 12
          }
        }
      }
    }
  ]
}
